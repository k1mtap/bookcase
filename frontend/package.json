{
  "name": "@full-stack-exercise/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "test": "CI=true react-scripts test --colors",
    "eject": "react-scripts eject",
    "clean": "rm -rf ./dist",
    "compile": "mv build dist",
    "build": "npm run clean && react-scripts build && npm run compile"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
